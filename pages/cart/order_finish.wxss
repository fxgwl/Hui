/* pages/cart/order_finish.wxss */
page {
  height: auto;
  background-color: #f2f5fa;
  color: #333;
  font-weight: 600;
  font-size: 30rpx;
  padding: 20rpx 0 100rpx 0;
}
.details_a {
  border-radius: 20rpx;
  background-color: #fff;
  margin: 0rpx 30rpx 20rpx;
}
.details_a .flex1 {
  height: 100rpx;
  padding: 0 50rpx;
}
.details_a .flex1 text:first-child {
  font-weight: 500;
}
.details_a .local {
  border-bottom: 2rpx solid #f2f5fa;
}
.details_a .local text:first-child {
  text-indent: 1em;
}
.details_a .time1 {
  color: #1074bc;
  margin-left: 10rpx;
  box-sizing: border-box;
}
.details_b {
  height: auto;
  background-color: #fff;
  border-radius: 20rpx;
  padding:0 30rpx 50rpx;
  margin: 0 30rpx;
}
.con1 {
  justify-content: start;
  border-bottom: 2rpx solid #f2f5fa;
  height: 90rpx;
}
.con1 image {
  width: 10rpx;
  height: 17rpx;
  margin-left: 15rpx;
  box-sizing: border-box;
}
.con2 {
  padding: 0 30rpx;
}
.bg1 {
  height: 340rpx;
  overflow: hidden;
}
.bg2 {
  height: auto;
  overflow: visible;
}
.con2_item {
  height: 170rpx;
}
.item1 {
  width: 120rpx;
  height: 120rpx;
}
.item2 {
  width: 350rpx;
  height: 120rpx;
}
.item2 .flex1 {
  justify-content: start;
  color: #1074bc;
  margin-top: 30rpx;
}
.item2 .del {
  color: #e22f2f;
  text-decoration: line-through;
  font-size: 24rpx;
  font-weight: 500;
  margin-left: 25rpx;
}
.item2 .del text {
  color: #666;
}
.item3 {
  font-weight: 500;
}
.con3 {
  margin: 25rpx 40rpx 0;
  height: 60rpx;
  background-color: #f2f5fa;
  justify-content: center;
  font-size: 24rpx;
  border-radius: 50rpx;
  font-weight: 500;
}
.con3 image {
  width: 17rpx;
  height: 10rpx;
  margin-left: 10rpx;
  box-sizing: border-box;
}
.details_c {
  padding: 0 30rpx;
  margin-bottom: 90rpx;
}
.details_c image {
  display: block;
  width: 240rpx;
  height: 30rpx;
  margin: 35rpx auto 30rpx;
  box-sizing: border-box;
}
.details_c text {
  display: block;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: #fff;
}


.details_d {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
}
.details_d1 {
  width: 50%;
  height: 100rpx;
  background-color: #fff;
  justify-content: start;
}
.details_d1 view:first-child {
  margin-right: 15rpx;
  padding-left: 30rpx;
  color: #1074bc;
  font-size: 32rpx;
}
/* .details_d1 view:last-child {
  color: #535353;
  font-size: 20rpx;
  font-weight: 500;
} */
.details_d2 {
  width: 50%;
  height: 100rpx;
  background-color: #1074bc;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  font-weight: 500;
}