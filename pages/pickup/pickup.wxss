/* pages/pickup/pickup.wxss */
.container{
  background: url(https://app.tjhengrun.cn/image/bg.png) no-repeat;
  background-size: 100% 100%;
  background-position: center;
  padding-bottom: 80rpx;
  overflow: hidden;
}
.pickup-box{
  width: 100%;
  height: 100%;
}
.pickup-first{
  width: 690rpx;
  height: 290rpx;
  background: url(https://app.tjhengrun.cn/image/pickup_bg.png) no-repeat;
  background-size: 100%;
  background-position: center;
  margin: 0 auto;
  margin-top: 20rpx;
}
.pickup-list{
  width: 690rpx;
  height: 295rpx;
  background: url(https://app.tjhengrun.cn/image/pickup_list.png) no-repeat;
  background-size: 100%;
  background-position: center;
  position: relative;
  margin: 0 auto;
  margin-top: -20rpx;
}
.pickup-number-list{
  position: absolute;
  top: 124rpx;
  left: 4%;
  width: 92%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.pickup-number-list view{
  width: 100rpx;
  height: 100%;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333333;
  font-weight: bold;
  background-color: #ececec;
  border-radius: 14rpx;
}
.pickup-tips-list{
  position: absolute;
  top: 224rpx;
  left: 4%;
  width: 92%;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  font-size: 18rpx;
  color: #333333;
}